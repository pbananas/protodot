; Engine configuration file.
; It's best edited using the editor UI and not directly,
; since the parameters that go here are not all obvious.
;
; Format:
;   [section] ; section goes between []
;   param=value ; assign values to parameters

config_version=5

[application]

config/name="_template"
run/main_scene="res://modes/main.tscn"
run/print_header=false
config/features=PackedStringArray("4.3", "Mobile")
boot_splash/bg_color=Color(0.040736, 0.040736, 0.040736, 1)
boot_splash/show_image=false
boot_splash/use_filter=false
config/icon="res://assets/icon.svg"

[autoload]

Events="*res://autoload/events.gd"
Config="*res://autoload/config.gd"
Persistence="*res://autoload/persistence.gd"
GameState="*res://autoload/game_state.gd"
Audio="*res://autoload/audio.gd"

[debug]

gdscript/warnings/unused_signal=0

[display]

window/size/viewport_width=320
window/size/viewport_height=180
window/size/window_width_override=1920
window/size/window_height_override=1080
window/stretch/mode="viewport"

[editor_plugins]

enabled=PackedStringArray("res://addons/AsepriteWizard/plugin.cfg")

[layer_names]

2d_physics/layer_1="player"
2d_physics/layer_2="walls"

[rendering]

textures/canvas_textures/default_texture_filter=0
renderer/rendering_method="mobile"
